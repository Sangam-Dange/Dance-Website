extends base.pug

block scripts
    script(src="../static/index.js")
        

block scripts
    style
        include ../static/style.css
        include ../static/stylecontact.css


block content
    div.container
        h1 Contact Us
        form(action="/contact" method = "post")
            input(type="text" class="MyInputs" name="name"  placeholder="Enter Your Name")
            input(type="phone" class="MyInputs" name="phone"  placeholder="Enter Your Phone Number")
            input(type="email" class="MyInputs" name="email"  placeholder="Enter Your Email")
            input(type="text" class="MyInputs" name="address"  placeholder="Enter Your Address")
            button( class="btn" )
                |submit

    
